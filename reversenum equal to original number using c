#include <stdio.h>

int main(void) {
  int num=181,reversed1,reversed2;
  int temp=num;
  while(temp>0){
    reversed1=reversed1*10+temp%10;
    temp/=10;
  }
  temp=reversed1;
  while(temp>0){
    reversed2=reversed2*10+temp%10;
    temp/=10;
  }
  if(reversed2==num){
    printf("True");
  }
  else{
    printf("False");
  }
   return 0;
}
