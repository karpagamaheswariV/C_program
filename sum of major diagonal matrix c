#include <stdio.h>

int main(void) {
  int a[3][3]={1,2,3,4,5,6,7,8,9},i,j;
  int sum=0;
  for(i=0;i<3;i++){
    for(j=0;j<3;j++){
      printf("%d ",a[i][j]);
    }printf("\n");
  }
  for(i=0;i<3;i++){
   
      sum=sum+a[i][i];
    
  }
  printf("The sum of the major diagonal:%d",sum);
  return 0;
}
