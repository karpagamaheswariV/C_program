#include <stdio.h>

int main(void) {
  int mark;
  printf("Enter the mark:\n");
  scanf("%d",&mark);
  if(mark<0 ||mark>100){
    printf("Invalid mark");
  }
  else if(mark>=90)
    printf("Grade A");
  else if(mark>=80)
    printf("Grade B");
  else if(mark>=70)
    printf("Grade C");
  else if(mark>=60)
    printf("Grade D");
  else
    printf("Grade E");

  return 0;
}
