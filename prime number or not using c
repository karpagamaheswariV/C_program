#include <stdio.h>
void prime(int n){           //using function
  int flag=0;
  for(int i=2;i<n;i++){
    if(n%i==0){
      flag=1;
      break;
    }
  }
  if(flag==1){
    printf("Not a prime number");
  }
  else{
    printf("Prime number");
  }
}
int main(void) {
  /*int n,flag=0;               //without using function
  scanf("%d",&n);
  for (int i=2;i<n;i++){
    if(n%i==0){
      flag=1;
      break;
    }
 }
