#include <stdio.h>

int main(void){
  int a[2][2]={1,2,3,4},b[2][2]={1,2,3,4};
  int i,j,k;
  for(i=0;i<2;i++){
    for(j=0;j<2;j++){
      if(a[i][j]==b[i][j]){
        k=1;
      }
      else{
        k=0;
        break;
      }
   }
  }
    if(k=1){
        printf("Matrix are equal");}
      else{
        printf("Matrix are not equal");
      }
  return 0;
}
